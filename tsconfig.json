{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "jsxImportSource": "react",
    "paths": {
      "@*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "src/templates"
  ],
  "references": [
    { "path": "./src/content" }
  ]
}
